        function keyDownNumber() {
            var key;
            if (navigator.appName == 'Microsoft Internet Explorer')
                key = event.keyCode; else key = event.which
            if (!(key >= 48 && key <= 57) && key != 8 && key != 36 && key != 37) {
                event.returnValue = false;
            }
        }

